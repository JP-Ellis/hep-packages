         RowBox[{"i_", ",", "j_"}], "}"}]}], "]"}], "\[RuleDelayed]", " ",
      RowBox[{
       SubscriptBox["Y", "x"], "[",
       RowBox[{"i", ",", "j"}], "]"}]}], ",", " ",
     RowBox[{
      RowBox[{"\[Mu]", "[",
       RowBox[{"{", "__", "}"}], "]"}], "\[RuleDelayed]", "\[Mu]"}], ",",
     RowBox[{
      RowBox[{"h", "[",
       RowBox[{
        RowBox[{"{",
         RowBox[{"x_", ",", "__"}], "}"}], ",",
        RowBox[{"{",
         RowBox[{"i_", ",", "j_"}], "}"}]}], "]"}], "\[RuleDelayed]", " ",
      RowBox[{
       SubscriptBox["h", "x"], "[",
